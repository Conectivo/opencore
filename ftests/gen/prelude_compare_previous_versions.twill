#do an edit 

go ${base_url}/projects/${projname}
follow edit
fv edit_form title $projfullname
fv edit_form text ${projbody}${pagelink}
submit 'Save'
notfind error_404_title
notfind 'site error' i
find $projfullname
find $projbody

#put it back
go ${base_url}/projects/${projname}
follow edit
fv edit_form title $projfullname
fv edit_form text ${projoldbody}${pagelink}
submit 'Save'
notfind error_404_title
notfind 'site error' i
find $projfullname
find $projoldbody

