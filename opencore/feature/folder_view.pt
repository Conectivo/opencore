<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      lang="en"
      metal:use-macro="here/@@standard_macros/master"
      i18n:domain="opencore">

<body>
<div metal:fill-slot="content"
     tal:define="features context/objectIds">
  <ul tal:condition="features">
    <li tal:repeat="feature features">
      <tal:comment replace="nothing">
      </tal:comment>
      <a tal:attributes="href string:${context/absolute_url}/${feature}"
         tal:content="feature" />
    </li>
  </ul>
  <p>
    <a class="oc-actionLink"
       tal:attributes="href string:${context/absolute_url}/create">
      Add</a>
    a new featured project
  </p>
</div>

</body>
</html>
