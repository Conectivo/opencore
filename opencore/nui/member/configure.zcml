<configure xmlns="http://namespaces.zope.org/zope"
	   xmlns:browser="http://namespaces.zope.org/browser"
	   xmlns:five="http://namespaces.zope.org/five">


  <!-- TODO write and run a migration and then use
     for="opencore.interfaces.IMemberFolder"
  -->
  <!-- TODO fix permissions -->

  <browser:page
     for="opencore.siteui.interfaces.IMemberFolder"
     class="opencore.nui.member.MemberPreferences"
     permission="zope.Public"
     name="preferences"
     template="preferences.pt"
     />


  <browser:page
     for="opencore.siteui.interfaces.IMemberFolder"
     class=".view.ProfileEditView"
     permission="zope.Public"
     name="profile-edit"
     template="profile-edit.pt"
     />


  <browser:page
     for="opencore.siteui.interfaces.IMemberFolder"
     attribute="handle_form"
     class=".view.ProfileEditView"
     permission="zope.Public"
     name="handle-form"
     />

 
  <browser:page
     for="opencore.siteui.interfaces.IMemberFolder"
     class=".view.ProfileView"
     permission="zope.Public"
     name="profile"
     template="profile.pt"
     />

</configure>
