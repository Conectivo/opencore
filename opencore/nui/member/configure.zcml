<configure xmlns="http://namespaces.zope.org/zope"
	   xmlns:browser="http://namespaces.zope.org/browser"
	   xmlns:five="http://namespaces.zope.org/five">

  <!-- member preferences -->
  <!-- @@ TODO need a specific interface for a member folder -->
  <browser:page
     for="*"
     class="opencore.nui.member.YourProjectsView"
     permission="zope.Public"
     name="preferences"
     template="preferences.pt"
     />

  <!-- member-profile-edit -->
  <browser:page
     for="*"
     class="..base.BaseView"
     permission="zope.Public"
     name="profile-edit"
     template="profile-edit.pt"
     />

  <!-- member-profile -->
  <browser:page
     for="*"
     class="..base.BaseView"
     permission="zope.Public"
     name="profile"
     template="profile.pt"
     />

  <!-- member-contact -->
  <browser:page
     for="*"
     class="..base.BaseView"
     permission="zope.Public"
     name="contact"
     template="contact.pt"
     />

</configure>
