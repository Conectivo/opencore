    <div metal:fill-slot="content">
      <div id="oc-content-main" class="oc-content-main-fullWidth">

        <div class="oc-wiki">
          <div class="oc-headingBlock oc-wiki-headingBlock">
            <ul class="oc-bcrumb">
              <!-- <li><a href="#">My Great Project</a> /</li> -->
              <li>
                <span class="oc-bcrumb-current" tal:content="view/pagetitle"></span>
              </li>
            </ul>
          </div>

          <div metal:use-macro="here/@@wiki_macros/tabs"/>

          <div class="oc-wiki-content" tal:content="structure here/getText"/>

        </div><!-- end .oc-wiki -->
      </div><!-- end #oc-content-main -->
    </div><!-- end content slot -->
  </body>
</html>
