<!-- title of and tabs for project content (view/edit/history) -->
<div id="content-title">
  <span id="content-title-pagetitle" tal:content="structure view/renderPageTitle"/>
  <ul class="oc-tabs">
    <li tal:repeat="tab view/tabs" tal:attributes="class tab/cssclass">
      <a tal:attributes="href tab/url" tal:content="tab/name" />
    </li>
  </ul>
</div>

<!-- project content -->
<div id="content-main" tal:content="structure view/renderProjectContent" />
