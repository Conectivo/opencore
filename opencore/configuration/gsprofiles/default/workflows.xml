   <bound-workflow workflow_id="openplans_team_workflow"/>
  </type>
  <type type_id="Team Membership">
   <bound-workflow workflow_id="openplans_team_membership_workflow"/>
  </type>
  <type type_id="TeamsTool">
   <bound-workflow workflow_id="openplans_team_workflow"/>
  </type>
  <type type_id="Topic">
   <bound-workflow workflow_id="folder_workflow"/>
  </type>
 </bindings>
</object>
