  <type type_id="ATSortCriterion"/>
  <type type_id="Discussion Item"/>
  <type type_id="FileAttachment"/>
  <type type_id="Folder">
   <bound-workflow workflow_id="folder_openplans_workflow"/>
  </type>
  <type type_id="ImageAttachment"/>
  <type type_id="Large Plone Folder">
