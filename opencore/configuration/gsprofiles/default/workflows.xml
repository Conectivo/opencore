  </type>
  <type type_id="Member">
   <bound-workflow workflow_id="openplans_member_workflow"/>
  </type>
  <type type_id="OpenMember">
   <bound-workflow workflow_id="openplans_member_workflow"/>
  </type>
  <type type_id="OpenMembership">
